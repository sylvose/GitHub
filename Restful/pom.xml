  <build>
    <sourceDirectory>${project.basedir}/src/main/java</sourceDirectory>
    <scriptSourceDirectory>${project.basedir}/src/scripts</scriptSourceDirectory>
    <testSourceDirectory>${project.basedir}/src/test/java</testSourceDirectory>
    <outputDirectory>${project.basedir}/src/main/webapp/WEB-INF/classes</outputDirectory>
    <testOutputDirectory>${project.basedir}/src/main/webapp/WEB-INF/classes</testOutputDirectory>
    <defaultGoal>install</defaultGoal>
    <finalName>${project.artifactId}-${project.version}</finalName>
    <plugins>
       <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.0</version>
        <inherited>true</inherited>
        <configuration>
          <source>${maven.compiler.version}</source>
          <target>${maven.compiler.version}</target>
          <compilerVersion>${maven.compiler.version}</compilerVersion>
          <encoding>${project.build.sourceEncoding}</encoding>
          <verbose>true</verbose>
          <optimize>true</optimize>
          <fork>true</fork>
          <failOnError>true</failOnError>
          <showDeprecation>true</showDeprecation>
          <showWarnings>true</showWarnings>
          <meminitial>1024m</meminitial>
          <maxmem>2048m</maxmem>
        </configuration>
        <executions>
	          <execution>
	            <id>verify</id>
	            <phase>verify</phase>
	            <goals>
		              <goal>compile</goal>
		              <goal>testCompile</goal>
	            </goals>
	          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.jvnet.ws.wadl</groupId>
        <artifactId>wadl-client-plugin</artifactId>
        <version>1.1.4</version>
        <executions>
          <execution>
            <goals>
              <goal>generate</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <sourceDirectory>../src/main/java</sourceDirectory>
          <packageName>com.gabon.info</packageName>
          <autopackaging>false</autopackaging>
          <customizations>
            <customization>../com/gabon/info/binding.xjb</customization>
          </customizations>
          <customClassNames>
            <property>
              <name>http://www.bbc.co.uk</name>
              <value>BBCSchedule</value>
            </property>
         </customClassNames>
        </configuration>
    </plugin>
	<plugin>
		<groupId>org.jvnet.jaxb2.maven2</groupId>
		<artifactId>maven-jaxb2-plugin</artifactId>
		<version>0.8.0</version>
		<executions>
			<execution>
				<phase>generate-sources</phase>
				<goals>
					<goal>generate</goal>
				</goals>
			</execution>
		</executions>
		<configuration>
			<generatePackage>com.gabon.info.model</generatePackage>
			<schemaDirectory>${project.basedir}/src/main/resources/com/gabon/info</schemaDirectory>
			<includeSchemas>
				<includeSchema>**/*.xsd</includeSchema>
			</includeSchemas>
			<extension>true</extension>
			<strict>false</strict>
			<verbose>true</verbose>
		</configuration>
	</plugin>
 	   <plugin>
            <groupId>com.sun.jersey.contribs</groupId>
            <artifactId>maven-wadl-plugin</artifactId>
            <version>${jersey-version}</version>
            <configuration>
                <wadlFile>${project.build.directory}/${project.build.finalName}.wadl</wadlFile>
                <formatWadlFile>true</formatWadlFile>
                <baseUri>${baseUri}</baseUri>
                <packagesResourceConfig>
                    <param>com.gabon.info.resource</param>
                </packagesResourceConfig>
                <wadlGenerators>
                    <wadlGeneratorDescription>
                        <className>com.sun.jersey.server.wadl.generators.WadlGeneratorApplicationDoc</className>
                        <properties>
                            <property>
                                <name>applicationDocsFile</name>
                                <value>${project.basedir}/src/main/resources/com/gabon/info/doc/application-doc.xml</value>
                            </property>
                        </properties>
                    </wadlGeneratorDescription>
                    <wadlGeneratorDescription>
                        <className>com.sun.jersey.server.wadl.generators.WadlGeneratorGrammarsSupport</className>
                        <properties>
                            <property>
                                <name>grammarsFile</name>
                                <value>${project.basedir}/src/main/resources/com/gabon/info/doc/application-grammars.xml</value>
                            </property>
                        </properties>
                    </wadlGeneratorDescription>
                    <wadlGeneratorDescription>
                        <className>com.sun.jersey.server.wadl.generators.resourcedoc.WadlGeneratorResourceDocSupport</className>
                        <properties>
                            <property>
                                <name>resourceDocFile</name>
                                <value>${project.build.directory}/resourcedoc-${project.build.finalName}.xml</value>
                            </property>
                        </properties>
                    </wadlGeneratorDescription>
                    <!--
                    <wadlGeneratorDescription>
                            <className>com.sun.jersey.samples.generatewadl.util.ExampleWadlGenerator</className>
                            <properties>
                                <property>
                                    <name>resourceDocFile</name>
                                    <value>${project.build.directory}/resourcedoc-${project.build.finalName}.xml</value>
                                </property>
                            </properties>
                   </wadlGeneratorDescription>
                   -->
                </wadlGenerators>
            </configuration> 
            <executions>
                <execution>
                    <id>wadl</id>
                    <phase>verify</phase>
                    <goals>
                        <goal>generate</goal>
                    </goals>
                    <configuration>
		                <wadlFile>${project.build.directory}/${project.build.finalName}.wadl</wadlFile>
                        <formatWadlFile>true</formatWadlFile>
                        <baseUri>${baseUri}</baseUri>
                        <packagesResourceConfig>
                            <param>com.gabon.info.resource</param>
                        </packagesResourceConfig>
                        <wadlGenerators>
                            <wadlGeneratorDescription>
                                <className>com.sun.jersey.server.wadl.generators.WadlGeneratorApplicationDoc</className>
                                <properties>
                                    <property>
                                        <name>applicationDocsFile</name>
                                        <value>${project.basedir}/src/main/resources/com/gabon/info/doc/application-doc.xml</value>
                                    </property>
                                </properties>
                            </wadlGeneratorDescription>
                            <wadlGeneratorDescription>
                                <className>com.sun.jersey.server.wadl.generators.WadlGeneratorGrammarsSupport</className>
                                <properties>
                                    <property>
                                        <name>grammarsFile</name>
                                        <value>${project.basedir}/src/main/resources/com/gabon/info/doc/application-grammars.xml</value>
                                    </property>
                                </properties>
                            </wadlGeneratorDescription>
                        </wadlGenerators>
                    </configuration>
                </execution>
            </executions>             
    </plugin>
	<plugin>
		<groupId>org.apache.maven.plugins</groupId>
