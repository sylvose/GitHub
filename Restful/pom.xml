               </tasks>
           </configuration>
          <executions>
              <execution>
                  <phase>verify</phase>
                  <goals>
                      <goal>run</goal>
                  </goals>
              </execution>
          </executions>
      </plugin>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <version>2.4.1</version>
        <executions>
          <execution>
            <id>auto-clean</id>
            <phase>clean</phase>
            <goals>
              <goal>clean</goal>
            </goals>
            <inherited>false</inherited>
            <configuration>
              <filesets>
                <fileset>
                  <directory>${project.version}</directory>
                </fileset>
              </filesets>
            </configuration>
          </execution>
        </executions>
        <inherited>true</inherited>
      </plugin>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <version>2.9</version>
        <inherited>true</inherited>
        <configuration>
          <downloadSources>true</downloadSources>
          <downloadJavadocs>true</downloadJavadocs>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.googlecode.mavennatives</groupId>
        <artifactId>maven-nativedependencies-plugin</artifactId>
        <version>0.0.6</version>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <version>1.8</version>
        <executions>
          <execution>
            <id>add-source</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>add-source</goal>
            </goals>
            <configuration>
              <sources>
                <source>${project.basedir}/src/main/java</source>
              </sources>
            </configuration>
          </execution>
          <execution>
            <id>add-test-source</id>
            <phase>generate-test-sources</phase>
            <goals>
              <goal>add-test-source</goal>
            </goals>
            <configuration>
              <sources>
                <source>${project.basedir}/src/test/java</source>
              </sources>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-scm-plugin</artifactId>
        <version>1.6</version>
        <inherited>true</inherited>
        <configuration>
          <includes>${maven.scm.includes}</includes>
          <message>Deployed by jenkins (build #${BUILD_NUMBER})</message>
          <basedir>${project.build.directory}/${project.artifactId}</basedir>
          <checkoutDirectory>${project.build.directory}/${project.artifactId}</checkoutDirectory>
          <connectionUrl>scm:git:git@localhost:7000/${project.artifactId}.git</connectionUrl>
          <developerConnectionUrl>scm:git:git@localhost:7000/${project.artifactId}.git</developerConnectionUrl>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>buildnumber-maven-plugin</artifactId>
        <version>1.1</version>
        <executions>
          <execution>
            <id>buildNumber</id>
            <phase>verify</phase>
            <goals>
              <goal>create</goal>
            </goals>
            <configuration>
              <format>{0,number}.{1,number}.{2,number}</format>
              <items>
                <item>buildNumber0</item>
                <item>buildNumber1</item>
                <item>buildNumber2</item>
              </items>
            </configuration>
          </execution>
          <execution>
            <id>timestamp</id>
            <phase>verify</phase>
            <goals>
              <goal>create-timestamp</goal>
            </goals>
            <configuration>
              <format>At {1,time} on {1,date}</format>
              <items>
                <item implementation="java.lang.Integer">7</item>
                <item>timestamp</item>
              </items>
            </configuration>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
          <doCheck>true</doCheck>
          <doUpdate>true</doUpdate>
          <shortRevisionLength>5</shortRevisionLength>
          <useLastCommittedRevision>true</useLastCommittedRevision>
          <getRevisionOnlyOnce>true</getRevisionOnlyOnce>
          <revisionOnScmFailure>true</revisionOnScmFailure>
          <useLastCommittedRevision>true</useLastCommittedRevision>
          <shortRevisionLength>5</shortRevisionLength>
          <buildNumberPropertiesFileLocation>${project.basedir}/src/main/resources/com/gabon/info/buildNumber.properties</buildNumberPropertiesFileLocation>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <version>2.10</version>
        <executions>
          <execution>
            <id>verify</id>
            <phase>verify</phase>
            <goals>
              <goal>check</goal>
              <goal>checkstyle</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
		          <enableRulesSummary>true</enableRulesSummary>
		          <enableSeveritySummary>true</enableSeveritySummary>
		          <consoleOutput>true</consoleOutput>
		          <failOnViolation>false</failOnViolation>
		          <failOnError>false</failOnError>
		          <includeTestSourceDirectory>true</includeTestSourceDirectory>
		          <logViolationsToConsole>true</logViolationsToConsole>
		          <useFile>${project.build.directory}/checkstyle/checkstyle.log</useFile>
		          <outputFile>${project.build.directory}/checkstyle/checkstyle-result.xml</outputFile>
		          <cacheFile>${project.build.directory}/checkstyle/checkstyle-cachefile.log</cacheFile>
		          <violationSeverity>info</violationSeverity>
		          <headerLocation>${project.basedir}/src/main/resources/com/gabon/info/LICENSE</headerLocation>
		          <configLocation>${project.basedir}/src/main/resources/com/gabon/info/checkstyle.xml</configLocation>
		          <sourceDirectory>${project.build.sourceDirectory}</sourceDirectory>
		          <testSourceDirectory>${project.build.testSourceDirectory}</testSourceDirectory>
		          <outputDirectory>${project.build.directory}/checkstyle</outputDirectory>
        </configuration> 
      </plugin>
      <plugin>
       <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>3.0.1</version>
        <executions>
          <execution>
            <id>verify</id>
            <phase>verify</phase>
            <goals>
              <goal>pmd</goal>
              <goal>cpd</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
          <aggregate>true</aggregate>
          <linkXref>true</linkXref>
          <sourceEncoding>${project.build.sourceEncoding}</sourceEncoding>
          <minimumTokens>100000</minimumTokens>
          <verbose>true</verbose>
          <rulesets>
            <ruleset>${project.basedir}/src/main/resources/com/gabon/info/pmd.xml</ruleset>
          </rulesets>
          <includes>
            	<include>com/gabon/info/**/*.java</include>
          </includes>
          <excludes>
            <exclude>**/Mock.*</exclude>
            <exclude>**/Dummy.*</exclude>
            <exclude>**/*Mock.java</exclude>
            <exclude>**/*Dummy.java</exclude>
          </excludes>
          <targetJdk>${maven.compiler.version}</targetJdk>
          <formats>
            <format>html</format>
          </formats>
          <includeTests>true</includeTests>
          <outputDirectory>${project.build.directory}/pmd</outputDirectory>
        </configuration>
      </plugin>
        <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <id>verify</id>
            <phase>verify</phase>
            <goals>
              <goal>check</goal>
              <goal>findbugs</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
          <effort>Max</effort>
          <threshold>High</threshold>
          <includeTests>true</includeTests>
          <failOnError>true</failOnError>
          <excludeFilterFile>${project.basedir}/src/main/resources/com/gabon/info/findbugs-exclude.xml</excludeFilterFile>
          <includeFilterFile>${project.basedir}/src/main/resources/com/gabon/info/findbugs-include.xml</includeFilterFile>
          <visitors>FindDeadLocalStores,UnreadFields</visitors>
          <omitVisitors>FindDeadLocalStores, UnreadFields</omitVisitors>
          <onlyAnalyze>com.gabon.info*</onlyAnalyze>
          <debug>true</debug>
          <relaxed>true</relaxed>
          <xmlOutput>true</xmlOutput>
          <findbugsXmlOutput>true</findbugsXmlOutput>
          <findbugsXmlWithMessages>true</findbugsXmlWithMessages>
          <classFilesDirectory>${project.basedir}/src/main/java</classFilesDirectory>
          <outputDirectory>${project.build.directory}/findbugs</outputDirectory>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <version>2.5.2</version>
        <executions>
          <execution>
            <id>package</id>
            <phase>package</phase>
            <goals>
              <goal>clean</goal>
              <goal>cobertura</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
          <instrumentation>
            <ignores>
              <ignore>org.*</ignore>
            </ignores>
            <excludes>
              <exclude>org/**/*.class</exclude>
            </excludes>
          </instrumentation>
          <check>
            <branchRate>85</branchRate>
            <lineRate>85</lineRate>
            <haltOnFailure>true</haltOnFailure>
            <totalBranchRate>85</totalBranchRate>
            <totalLineRate>85</totalLineRate>
            <packageLineRate>85</packageLineRate>
            <packageBranchRate>85</packageBranchRate>
            <regexes>
              <regex>
                <pattern>com.gabon.info.*</pattern>
                <branchRate>90</branchRate>
                <lineRate>80</lineRate>
              </regex>
            </regexes>
          </check>
          <formats>
            <format>html</format>
          </formats>
          <forceMojoExecution>true</forceMojoExecution>
          <attach>true</attach>
          <aggregate>true</aggregate>
          <omitGplFiles>true</omitGplFiles>
          <outputDirectory>${project.build.directory}/cobertura</outputDirectory>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <id>compile</id>
            <phase>compile</phase>
            <goals>
              <goal>exploded</goal>
            </goals>
          </execution>
          <execution>
            <id>verify</id>
            <phase>verify</phase>
            <goals>
              <goal>war</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
          <failOnError>true</failOnError>
          <followSymLinks>true</followSymLinks>
          <verbose>true</verbose>
          <failOnMissingWebXml>false</failOnMissingWebXml>
          <warSourceExcludes>**/*.bak, **/*Test*, **/*Mock.*, **/*Dummy.*, **/*Mock.java, **/*Dummy.java, **/*test-context.xml, **/com/gabon/info/test/**/*.*</warSourceExcludes>
          <packagingIncludes>**/*.class, img/*.gif, img/*.png, META-INF/*.xml, WEB-INF/lib/*.jar, **/*.htm*, **/*.jsp, **/*.xml, **/*.xsd, **/*.wadl, **/*.wsdl</packagingIncludes>
          <webResources>
              	   <resource>
			              <directory>${project.basedir}/src/main/webapp/img</directory>
			              <filtering>true</filtering>
			              <includes>
			                	<include>**/*.gif</include>
			                	<include>**/*.png</include>
			              </includes>
			       </resource>
		           <resource>
			              <directory>${project.basedir}/src/main/java/META-INF</directory>
			              <filtering>true</filtering>
			              <includes>
			                	<include>**/*.xml</include>
			                	<include>**/*.jsp</include>
			                	<include>**/*.htm</include>
			                	<include>**/*.html</include>
			                	<include>**/*.dtd</include>
			                	<include>**/*.xsd</include>
			                	<include>**/*.wadl</include>
			                	<include>**/*.wsdl</include>
			                	<include>**/*.MF</include>
			              </includes>
			              <excludes>
			              			<exclude>**/*.jar</exclude>
			              			<exclude>**/*.bak</exclude>
			              </excludes>
		           </resource>
		           <resource>
			              <directory>${project.basedir}/src/main/webapp/WEB-INF/lib</directory>
			              <filtering>false</filtering>
			              <includes>
			                	<include>**/*.jar</include>
			              </includes>
			              <excludes>
			              		<exclude>**/*.xml</exclude>
			              		<exclude>**/*.jsp</exclude>
			              		<exclude>**/*.htm</exclude>
			              		<exclude>**/*.html</exclude>
			                	<exclude>**/*.dtd</exclude>
			                	<exclude>**/*.xsd</exclude>
			                	<exclude>**/*.wadl</exclude>
			                	<exclude>**/*.wsdl</exclude>
			                	<exclude>**/*.MF</exclude>
			              </excludes>
		           </resource>
		  </webResources>
          <archiveClasses>false</archiveClasses>
          <attachClasses>true</attachClasses>
          <cacheFile>${project.build.directory}/webapp-cache.xml</cacheFile>
          <useCache>false</useCache>
          <classesClassifier>classes</classesClassifier>
          <classifier></classifier>
          <containerConfigXML>${project.basedir}/src/main/java/META-INF/context.xml</containerConfigXML>
          <warName>${project.build.finalName}</warName>
          <webXml>${project.basedir}/src/main/webapp/WEB-INF/web.xml</webXml>
          <warSourceExcludes>${project.basedir}/src/test/java/**</warSourceExcludes>
          <webappDirectory>${project.build.directory}/${project.build.finalName}</webappDirectory>
          <workDirectory>${project.build.directory}</workDirectory>
          <outputDirectory>${project.build.directory}/classes</outputDirectory>
          <archive>
            <index>true</index>
            <manifest>
              <addClasspath>true</addClasspath>
              <classpathLayoutType>repository</classpathLayoutType>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
              <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
              <classpathMavenRepositoryLayout>true</classpathMavenRepositoryLayout>
            </manifest>
            <manifestEntries>
              <url>${project.url}</url>
              <Maven-Version>${maven.version}</Maven-Version>
              <Manifest-Version>${project.version}</Manifest-Version>
              <Archiver-Version>${archiver.version}</Archiver-Version>
              <Specification-Title>${project.name}</Specification-Title>
              <Specification-Version>${project.version}</Specification-Version>
              <Specification-Vendor>${project.organization.name}</Specification-Vendor>
              <Implementation-Build>${project.buildNumber}</Implementation-Build>
              <Implementation-Title>${project.name}</Implementation-Title>
              <Implementation-Vendor>${project.organization.name}</Implementation-Vendor>
              <Implementation-Vendor-Id>${project.groupId}</Implementation-Vendor-Id>
              <Implementation-Version>${project.version}</Implementation-Version>
              <Created-By>${user.name}</Created-By>
              <Built-By>${user.name}</Built-By>
              <Build-Jdk>${java.version}</Build-Jdk>
              <Built-OS>${os.name}</Built-OS>
              <Build-Date>${timestamp}</Build-Date>
              <SCM>${project.buildNumber}</SCM>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.github.goldin</groupId>
        <artifactId>jenkins-maven-plugin</artifactId>
        <version>0.2.5</version>
        <executions>
          <execution>
            <id>generate-jenkins-jobs</id>
            <phase>compile</phase>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <generationPom>http://github.com/sylvose/${project.artifactId}/pom.xml</generationPom>
              <jenkinsUrl>http://localhost:8000/</jenkinsUrl>
              <outputDirectory>${user.home}/.jenkins/jobs</outputDirectory>
              <jobs>
                <job>
                  <id>${project.name}</id>
                  <displayName>${project.name}</displayName>
                  <scmType>git</scmType>
                  <mavenName>apache-maven-3.0.4</mavenName>
                  <jdkName>jdk1.7.0_21</jdkName>
                  <abstract>true</abstract>
                  <mavenOpts>-Xms2048m -Xmx4096m -XX:PermSize=2048m -XX:MaxPermSize=4096m -XX:+UseParallelGC</mavenOpts>
                  <daysToKeep>3</daysToKeep>
                  <numToKeep>7</numToKeep>
                  <useUpdate>true</useUpdate>
                  <mavenGoals>-B -U -e clean test install</mavenGoals>
                  <archivingDisabled>true</archivingDisabled>
                  <buildOnSNAPSHOT>false</buildOnSNAPSHOT>
                  <doRevert>true</doRevert>
                  <authToken>AuthenticationToken</authToken>
                  <gitHubUrl>https://github.com/sylvose/GitHub.git/${project.artifactId}</gitHubUrl>
                  <incrementalBuild>true</incrementalBuild>
                  <quietPeriod>30</quietPeriod>
                  <scmCheckoutRetryCount>5</scmCheckoutRetryCount>
                  <localRepo>${user.home}/.m2/repository</localRepo>
                  <mail>
                    <recipients>asylvose@yahoo.fr</recipients>
                    <sendForUnstable>true</sendForUnstable>
                    <sendToIndividuals>true</sendToIndividuals>
                  </mail>
                  <repositories>
                    <repository>
                      <remote>git@github.com:sylvose/GitHub.git</remote>
                      <gitName>origin</gitName>
                      <gitRefspec>+refs/heads/*:refs/remotes/origin/*</gitRefspec>
                      <gitBranch>master</gitBranch>
                    </repository>
                  </repositories>
                  <trigger>
                    <type>timer</type>
                    <description>Runs every midnight except Saturday</description>
                    <expression>0 0 * * 0-5</expression>
                  </trigger>
                </job>
              </jobs>
            </configuration>
          </execution>
        </executions>
      </plugin>
