	<plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-antrun-plugin</artifactId>
          <configuration>
               <tasks>
                   <echo message="Copy ${project.build.finalName}.wadl and resourcedoc-${project.build.finalName}.xml to folder : ${project.basedir}/src/main/resources/com/gabon/info/doc"/>
                   <copy file="${project.build.directory}/${project.build.finalName}.wadl" todir="${project.basedir}/src/main/webapp"/>
